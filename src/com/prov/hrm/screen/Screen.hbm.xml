<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.prov.hrm.screen.Screen" table="tblscreen">
        <id name="screenId" type="java.lang.Integer">
            <column name="screen_id" />
            <generator class="identity" />
        </id>
        <property name="screenName" type="string">
            <column name="screen_name" length="45" not-null="true" />
        </property>
        <property name="insertBy" type="java.lang.Integer">
            <column name="insert_by" />
        </property>
        <property name="insertDate" type="string">
            <column name="insert_date" length="0" />
        </property>
        <property name="updateBy" type="java.lang.Integer">
            <column name="update_by" />
        </property>
        <property name="updateDate" type="string">
            <column name="update_date" length="0" />
        </property>
        <property name="deleteFlag" type="java.lang.Boolean">
            <column name="delete_flag" />
        </property>
    </class>
</hibernate-mapping>
