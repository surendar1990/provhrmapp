        </id>
        <property name="organizationId" type="int">
            <column name="organization_id" not-null="true" />
        </property>
        <property name="leavetypeSchemeName" type="string">
            <column name="leavetype_scheme_name" not-null="true" />
        </property>
        <property name="leavetypeSchemeDescription" type="string">
            <column name="leavetype_scheme_description" not-null="true" />
        </property>
        <property name="insertBy" type="java.lang.Integer">
            <column name="insert_by" />
        </property>
        <property name="insertDate" type="string">
            <column name="insert_date" length="0" />
        </property>
        <property name="updateBy" type="java.lang.Integer">
            <column name="update_by" />
        </property>
        <property name="updateDate" type="string">
            <column name="update_date" length="0" />
        </property>
        <property name="deleteFlag" type="java.lang.Boolean">
            <column name="delete_flag" />
        </property>
                 
                </class>
</hibernate-mapping>
